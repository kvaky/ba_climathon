{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start-dev-client": "expo start --dev-client",
    "start-production": "expo start --no-dev --minify",
    "android": "expo start --localhost --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "check-eslint": "eslint --max-warnings 0 ./components ./hooks ./navigation ./screens ./translations ./utils ./App.tsx",
    "check-ts": "tsc --noEmit --skipLibCheck",
    "check-code": "yarn check-eslint && yarn check-ts",
    "create-production-apk": "expo build:android --release-channel production -t apk --no-publish",
    "create-production-ipa": "expo build:ios --release-channel production -t archive --no-publish",
    "publish-production": "eas update --branch production",
    "publish-staging": "eas update --branch staging",
    "publish-development": "eas update --branch development",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo"
  },
